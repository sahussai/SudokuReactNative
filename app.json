{
  "expo": {
    "name": "Sudoku",
    "slug": "react-native-course",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/background_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/title2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myApp.playSudoku"
    },
    "android": {
      "icon": "./assets/background_icon.png",
      "edgeToEdgeEnabled": true,
      "package": "com.abeer.hussain94.reactnativecourse"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0f333232-cf86-40bd-bfa2-dc3df137b662"
      }
    },
    "plugins": ["expo-font"],
    "assetBundlePatterns": ["**/*"]
  }
}
